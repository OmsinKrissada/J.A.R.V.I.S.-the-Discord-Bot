{
	"name": "jarvis",
	"version": "v3.2.0",
	"description": "A Discord utility/music bot.",
	"main": "Main.js",
	"private": true,
	"type": "commonjs",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "node .",
		"dev": "nodemon",
		"build": "rimraf bundle && esbuild src/**/*.ts --platform=node --format=cjs --minify --keep-names --outdir=bundle --outbase=src && copyfiles --flat package.json prisma/schema.prisma bundle && copyfiles --flat src/settings/* bundle/settings && copyfiles --flat resources/* bundle/resources",
		"upload": "scp -r bundle/* 192.168.1.37:~/host/jarvis"
	},
	"keywords": [],
	"author": "OmsinKrissada",
	"license": "MIT",
	"dependencies": {
		"@prisma/client": "5.3.1",
		"axios": "^1.5.0",
		"bufferutil": "^4.0.7",
		"chalk": "^4.1.2",
		"chatgpt": "^3.5.2",
		"date-fns": "^2.30.0",
		"discord.js": "^12.5.3",
		"js-yaml": "^4.1.0",
		"moment": "^2.29.4",
		"os-utils": "0.0.14",
		"prisma": "^5.3.1",
		"shoukaku": "3.4.0",
		"ts-node": "^10.9.1",
		"winston": "^3.10.0",
		"yt-search": "^2.10.4"
	},
	"devDependencies": {
		"@swc-node/register": "^1.6.7",
