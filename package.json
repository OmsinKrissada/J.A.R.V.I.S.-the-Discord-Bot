		"chokidar": "^3.5.3",
		"copyfiles": "^2.4.1",
		"esbuild": "^0.19.3",
		"ffmpeg-static": "^5.2.0",
		"nodemon": "^3.0.1",
		"rimraf": "^5.0.1",
		"typescript": "^5.2.2"
	}
}